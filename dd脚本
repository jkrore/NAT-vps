#!/bin/bash
#
# zram-config Universal Installer for Debian-based systems
#
# This script automatically handles:
# - Root or non-root user execution.
# - Installation of all required dependencies (curl, wget, tar, lzip, sudo).
# - Fetching the latest stable release from GitHub.
# - Installation, service enabling, å’Œ cleanup.

( \
    set -e; \
    \
    # --- Step 1: Determine execution privilege and define command prefix ---
    echo "--- æ­¥éª¤ 1/8: æ£€æŸ¥ç”¨æˆ·æƒé™ ---"; \
    CMD_PREFIX=""; \
    if [ "$(id -u)" -ne 0 ]; then \
        echo "æ£€æµ‹åˆ°ä¸ºæ™®é€šç”¨æˆ·ï¼Œå°†ä½¿ç”¨ 'sudo' æå‡æƒé™ã€‚"; \
        CMD_PREFIX="sudo"; \
        # Ensure sudo is installed for non-root users
        if ! command -v sudo >/dev/null; then \
            echo "é”™è¯¯ï¼šå½“å‰ä¸ºæ™®é€šç”¨æˆ·ï¼Œä½†ç³»ç»Ÿä¸­æœªå®‰è£… 'sudo'ã€‚è¯·å…ˆè”ç³»ç®¡ç†å‘˜å®‰è£…sudoã€‚" >&2; \
            exit 1; \
        fi; \
    else \
        echo "æ£€æµ‹åˆ°ä¸º root ç”¨æˆ·ï¼Œå°†ç›´æ¥æ‰§è¡Œå‘½ä»¤ã€‚"; \
    fi; \
    \
    # --- Step 2: Check and install dependencies ---
    echo "--- æ­¥éª¤ 2/8: æ£€æŸ¥å¹¶å®‰è£…ä¾èµ–å·¥å…· (curl, wget, tar, lzip, sudo) ---"; \
    PACKAGES_NEEDED=""; \
    for pkg in curl wget tar lzip sudo; do \
        if ! command -v "$pkg" >/dev/null; then \
            PACKAGES_NEEDED="$PACKAGES_NEEDED $pkg"; \
        fi; \
    done; \
    \
    if [ -n "$PACKAGES_NEEDED" ]; then \
        echo "æ£€æµ‹åˆ°ä¾èµ–å·¥å…·ç¼ºå¤±: $PACKAGES_NEEDED"; \
        echo "æ­£åœ¨å°è¯•è‡ªåŠ¨å®‰è£…..."; \
        $CMD_PREFIX apt-get update; \
        $CMD_PREFIX apt-get install -y $PACKAGES_NEEDED; \
    fi; \
    echo "ä¾èµ–æ£€æŸ¥å®Œæ¯•ã€‚"; \
    \
    # --- Step 3: Fetch the latest stable release URL ---
    echo "--- æ­¥éª¤ 3/8: æ­£åœ¨ä» GitHub API è·å–æœ€æ–°ç¨³å®šç‰ˆé“¾æ¥ ---"; \
    LATEST_RELEASE_URL=$(curl -s "https://api.github.com/repos/ecdye/zram-config/releases/latest" | grep "browser_download_url.*tar.lz" | grep -v ".asc" | cut -d : -f 2,3 | tr -d ',"' | xargs); \
    if [ -z "$LATEST_RELEASE_URL" ]; then \
        echo "é”™è¯¯ï¼šæ— æ³•è·å–æœ€æ–°ç‰ˆæœ¬çš„ä¸‹è½½é“¾æ¥ï¼Œè¯·æ£€æŸ¥ç½‘ç»œæˆ–ç¨åå†è¯•ã€‚" >&2; \
        exit 1; \
    fi; \
    echo "æˆåŠŸè·å–é“¾æ¥: $LATEST_RELEASE_URL"; \
    \
    # --- Step 4: Download the release ---
    echo "--- æ­¥éª¤ 4/8: æ­£åœ¨ä¸‹è½½æœ€æ–°ç¨³å®šç‰ˆ ---"; \
    FILE_NAME=$(basename "$LATEST_RELEASE_URL"); \
    wget -qO "$FILE_NAME" "$LATEST_RELEASE_URL"; \
    echo "ä¸‹è½½å®Œæˆ: $FILE_NAME"; \
    \
    # --- Step 5: Extract the archive ---
    echo "--- æ­¥éª¤ 5/8: æ­£åœ¨è§£å‹æ–‡ä»¶ ---"; \
    TEMP_DIR="zram-config-temp-$$"; \
    mkdir -p "$TEMP_DIR"; \
    tar -xf "$FILE_NAME" --strip-components=1 --directory="$TEMP_DIR"; \
    echo "è§£å‹å®Œæˆã€‚"; \
    \
    # --- Step 6: Run the installation script ---
    echo "--- æ­¥éª¤ 6/8: æ­£åœ¨æ‰§è¡Œå®‰è£…è„šæœ¬ ---"; \
    $CMD_PREFIX ./"$TEMP_DIR"/install.bash; \
    echo "å®‰è£…è„šæœ¬æ‰§è¡Œå®Œæ¯•ã€‚"; \
    \
    # --- Step 7: Enable and start the service ---
    echo "--- æ­¥éª¤ 7/8: å¯åŠ¨æœåŠ¡å¹¶è®¾ç½®ä¸ºå¼€æœºè‡ªå¯ ---"; \
    $CMD_PREFIX systemctl enable --now zram-config.service; \
    echo "æœåŠ¡å·²å¯åŠ¨å¹¶è®¾ç½®å¼€æœºè‡ªå¯ã€‚"; \
    \
    # --- Step 8: Cleanup ---
    echo "--- æ­¥éª¤ 8/8: æ¸…ç†ä¸´æ—¶æ–‡ä»¶å’Œç›®å½• ---"; \
    $CMD_PREFIX rm -f "$FILE_NAME"; \
    $CMD_PREFIX rm -rf "$TEMP_DIR"; \
    echo "æ¸…ç†å®Œæˆã€‚"; \
    \
    echo "--------------------------------------------------"; \
    echo "âœ… zram-config å·²æˆåŠŸå®‰è£…å¹¶è¿è¡Œï¼"; \
    echo "--------------------------------------------------"; \
    echo "å½“å‰æœåŠ¡çŠ¶æ€:"; \
    $CMD_PREFIX systemctl status zram-config.service --no-pager; \
    echo -e "\né…ç½®æ–‡ä»¶ä½äº /etc/ztabï¼Œæ‚¨å¯ä»¥æŒ‰éœ€ä¿®æ”¹ã€‚"; \
)
